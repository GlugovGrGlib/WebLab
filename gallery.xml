                color: #fff;
                font-size: 1.4rem;
                font-weight: 600;
                line-height: 1.0em;
                text-transform: uppercase;
                display: block;
              }

              #menu ul {
                float: left;
                margin: 0;
                height: 50px;
                list-style: none;
                padding-left: 110px;
              }



              #menu ul li {
                text-align: center;
                display: block;
                float: left;
                margin: 0 !important;
                padding: 0;
                position: relative;
                min-width: 140px;
                max-width: 180px;
                height: 50px;

                transition: background-color .15s ease-in-out;
              }

              #menu ul li a {
                width: 100%;
                height: 100%;
                display: table;

                transition: background-color .2s ease-in-out;
              }

              #menu ul li a span {
                line-height: 1em;
                width: 100%;
                padding: 0 20px;
                display: table-cell;
                vertical-align: middle;
              }

              #menu ul li li span {
                width: auto;
                padding: 0;
                display: block;
              }

              #menu ul li:hover {
                background-color: #be630e;
              }

              #menu ul li.current a span:after {
                content: "";
                height: 2px;
                background: #be630e;
                position: absolute;
                right: 0;
                bottom: 0;
                left: 0;
              }

              #menu ul li.current a span.live:after {
                display: none;
              }

              #menu ul li.current:hover a span:after,
              #menu ul li.current li a span:after {
                display: none;
              }

              #menu ul li:before {
                position: absolute;
                content: "";
                width: 1px;
                top: 15%;
                bottom: 15%;
                left: 0;
                background: linear-gradient(to bottom, rgba(120, 120, 120, 0) 0%, rgba(120, 120, 120, 1) 50%, rgba(120, 120, 120, 0) 100%);
              }


              #menu ul li a span span.live {
                text-indent: -999px;
                width: 30px;
                height: 22px;
                margin: 0 0 0 8px;
                padding: 0;
                background-size: auto 200%;
                display: inline-block;
                overflow: hidden;
                position: relative;
                top: -1px;
              }

              #menu ul li li:before {
                background: none;
              }

              #menu li.has-children ul {
                display: none;
                width: auto;
                height: auto;
                position: absolute;
                background-color: #403d3d;
                top: 50px;
                left: 0;
                padding-bottom: 20px;
                padding: 0;
              }

              #menu li.has-children:hover ul {
                display: block;
              }

              #menu li.has-children ul li {
                background-color: #403d3d;
                height: auto;
                margin-left: 0px;
                width: 100%;
              }

              #menu li.has-children ul li a {
                padding: 15px 10px;
              }

              #menu li.has-children ul li:hover {
                background-color: #be630e;
              }

              #menu li.current li.current {
                background-color: #212121;
              }

        .carousel-inner>.item>img,
        .carousel-inner>.item>a>img {
