              }

              body {
                background-color: #fff;
                color: #111;
                font-weight: 400;
                font-family: "Open Sans", sans-serif;
                margin: 0;
                padding: 0;

              }

              header {
                background: linear-gradient(150deg, black 10%, #333);
                ));
                background-size: auto 100%;
                z-index: 400;
                border-bottom: 1px solid #333;
                position: relative;
              }

              @keyframes anim {
                0% {
                  opacity: 0;
                }
                100% {
                  opacity: 0.9;
                }
              }

              #page.snap-menu header {
                height: 50px;
                position: fixed;
                top: 0;
                right: 0;
                left: 0;
                opacity: 0.9;
                transition: top .7s ease-in-out;
                animation: anim 1s;
              }

              header .wrapper {
                width: 1200px;
                height: 100px;
                margin: 0 auto;
                position: relative;

              }

              #page.snap-menu header .wrapper {
                height: 50px;
              }

              header .home {
                width: 104px;
                height: 95px;
                display: block;
                outline: 0;
                position: absolute;
                top: 0px;
                left: 0;
                z-index: 5000;
                transition-property: width, height, top;
                transition-duration: .25s;
                transition-timing-function: ease-in-out;
                display:none;
              }

              header .home:after {
                content: "";
                background-size: 100% 100%;
                position: absolute;
                top: 0;
                right: 0;
                bottom: 0;
                left: 0;
                z-index: 401;
                opacity: .5;
                transition: opacity .25s ease-in-out;
              }

              header .home span {
                text-indent: -999px;
                width: 100%;
                height: 100%;
                background: url(https://raw.githubusercontent.com/reactjs/redux/master/logo/logo.png) 0 0 no-repeat;
                background-size: 100% 100%;
                display: block;
                position: relative;
                z-index: 402;
                overflow: hidden;
              }

              #page.snap-menu header .home {
                width: 50px;
                height: 45px;
                top: 0;
              }

              #page.snap-menu header .home span {
                background-image: url(https://raw.githubusercontent.com/reactjs/redux/master/logo/logo.png);
              }

              /* !-- Header Utilities -- */

              #utils {
                float: right;
                position: relative;
                padding-top: 10px;
                height: 50px;
              }

              #page.snap-menu header #utils {
                display: none;
              }

              #utils a {
                color: #7e7e7e;
                float: left;
                font-size: 1.1rem;
                font-weight: 600;
                margin: 0 0 0 15px;
                padding: 9px 0 9px 34px;
                text-decoration: none;
                text-transform: uppercase;
                position: relative;
              }

              #utils a:hover,
              #utils a:focus {
                color: #be630e;
              }

              #utils a:after {
                font-weight: normal;
                font-size: 2.2rem;
                line-height: 1em;
                font-family: "icomoon", sans-serif;
                text-transform: none;
                text-align: center;
                width: 34px;
                position: absolute;
                top: .2em;
                left: 0;
              }

              #utils #search-form {
