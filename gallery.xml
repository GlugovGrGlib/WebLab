    <style type="text/css">


              * {
                line-height: 1.3em;
                text-size-adjust: 100%;
              }

              *,
              *:before,
              *:after {
                box-sizing: border-box;
              }

              html {
                font-size: 10px;
                opacity: 0;
              }

              .misc {
                font-size: 12px;
              }

              #page.snap-menu {
                padding-top: 100px;
              }

              .page-width {
