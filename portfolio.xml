          width: 104px;
          height: 95px;
          display: block;
          outline: 0;
          position: absolute;
          top: 0px;
          left: 0;
          z-index: 5000;
          transition-property: width, height, top;
          transition-duration: .25s;
          transition-timing-function: ease-in-out;
        }

        header .home:after {
          content: "";
          background-size: 100% 100%;
          position: absolute;
          top: 0;
          right: 0;
          bottom: 0;
          left: 0;
          z-index: 401;
          opacity: .5;
          transition: opacity .25s ease-in-out;
        }

        header .home span {
          text-indent: -999px;
          width: 100%;
          height: 100%;
