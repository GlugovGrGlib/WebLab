          }
          100% {
            opacity: 0.9;
          }
        }

        #page.snap-menu header {
          height: 50px;
          position: fixed;
          top: 0;
          right: 0;
          left: 0;
          opacity: 0.9;
          transition: top .7s ease-in-out;
          animation: anim 1s;
        }

        header .wrapper {
          width: 1200px;
          height: 100px;
          margin: 0 auto;
          position: relative;

        }

        #page.snap-menu header .wrapper {
          height: 50px;
        }

